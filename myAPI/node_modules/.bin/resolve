#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../_resolve@1.22.0@resolve/bin/resolve" "$@"
else 
  exec node  "$basedir/../_resolve@1.22.0@resolve/bin/resolve" "$@"
fi
